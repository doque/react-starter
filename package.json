{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "Starter Kit for React development with ES6 and CSS modules",
  "main": "dist/build.js",
  "private": true,
  "scripts": {
    "build": "webpack -p dist/build.min.js",
    "clean": "rm -rf dist",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint/bin/eslint.js . --ext .js,.jsx",
    "lint:scss": "stylelint/dist/cli.js **/*.scss --syntax scss",
    "start": "webpack-dev-server",
    "test": "mocha --opts .mocha.opts test/**/*spec.js",
    "test:watch": "mocha --opts .mocha.opts test/**/*.spec.js -w --watch-extensions js,jsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doque/react-starter.git"
  },
  "author": "Dominique Busser",
  "license": "UNLICENSED",
  "devDependencies": {
    "autoprefixer": "^6.3.4",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-css-modules-transform": "0.0.9",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-airbnb": "^1.1.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "fbjs": "^0.8.0-alpha.3",
    "file-loader": "^0.8.5",
    "ignore-styles": "^1.2.0",
    "jsdom": "^8.3.0",
    "loader-utils": "^0.2.12",
    "lodash": "^4.3.0",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.19",
    "postcss-loader": "^0.8.2",
    "react-addons-test-utils": "^0.14.7",
    "react-tools": "^0.13.3",
    "react-transform-hmr": "^1.0.2",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.2",
    "sasslint-loader": "0.0.1",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "stylelint": "^5.0.1",
    "stylelint-config-standard": "^5.0.0",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "component-ie": "^1.0.0",
    "moment": "^2.12.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
